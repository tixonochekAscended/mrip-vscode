{
  "name": "mini-rip",
  "displayName": "mini-rip",
  "description": "Highlighting for the esoteric, interpreted programming language mini-rip made by tixonochek.",
  "publisher": "tixonochek",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tixonochekAscended/mrip-vscode"
  },
  "homepage": "https://github.com/tixonochekAscended/mrip-vscode",
  "bugs": {
    "url": "https://github.com/tixonochekAscended/mrip-vscode/issues"
